{
  "name": "@epicgames/pixelstreamingfrontend",
  "version": "0.0.1",
  "description": "Frontend library for Pixel Streaming",
  "main": "dist/pixelstreamingfrontend.min.js",
  "types": "dist/ts/pixelstreamingfrontend.d.ts",
  "scripts": {
    "compile": "tsc --build --clean && tsc",
    "build": "tsc -noEmit && esbuild src/pixelstreamingfrontend.ts --bundle --minify --outfile=dist/pixelstreamingfrontend.min.js",
    "build-dev": "tsc -noEmit && esbuild src/pixelstreamingfrontend.ts --bundle --outfile=dist/pixelstreamingfrontend.js --sourcemap --target=es6",
    "build-webpack": "npx webpack",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "spellcheck": "cspell \"{README.md,.github/*.md,src/**/*.ts}\""
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "cspell": "^4.1.0",
    "esbuild": "0.16.17",
    "eslint": "^8.11.0",
    "ts-loader": "^9.4.2",
    "webpack-cli": "^5.0.1"
  },
  "dependencies": {
    "jss": "^10.9.2",
    "jss-plugin-global": "^10.9.2"
  }
}
